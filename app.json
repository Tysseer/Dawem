{
  "expo": {
    "scheme": "dawem",
    "name": "Dawem",
    "slug": "Dawem",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "plugins": [
      [

        "expo-notifications",
        {
          "icon": "./local/assets/dayBadge.png",
          "color": "#ffffff"
        }
      ]
    ],
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#EEEEEE"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.imuslim.dawem",
      "buildNumber": "1.0.5",
      "infoPlist": {
        "NSPhotoLibraryAddUsageDescriptio": "This app uses the location to detect your currency.",
        "NSPhotoLibraryUsageDescription": "This app uses the location to detect your currency."
      }
    },
    "android": {
      "package": "com.imuslim.dawem",
      "versionCode": 6,
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#EEEEEE"
      },
      "useNextNotificationsApi": true,
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "description": " Dawem is an application to make it easy to frequently read quran and plan your recitation so no verses are left unvisited for long ",
    "githubUrl": "https://github.com/tysseer/dawem",
    "sdkVersion": "44.0.0"
  }
}
